{
  "name": "@corail-factory/omnip-frontend",
  "description": "Your freelancing partner...",
  "author": "Pukar Sharma <sharmapukar217@gmail.com>",
  "sideEffects": false,
  "version": "0.1.6",
  "type": "module",
  "private": true,
  "scripts": {
    "commit": "cz",
    "dev": "nuxt dev",
    "prepare": "husky",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "dev:ssr": "cross-env DISABLE_SSR=false DISABLE_PWA=false nuxt dev",
    "dev:no-ssr": "cross-env DISABLE_SSR=true DISABLE_PWA=false nuxt dev",
    "dev:pwa": "cross-env DISABLE_PWA=false ENABLE_HTTPS=true nuxt dev",
    "dev:static": "cross-env DISABLE_SSR=true DISABLE_PWA=false nuxt dev",
    "dev:bare": "cross-env DISABLE_SSR=true DISABLE_PWA=true ENABLE_HTTPS=false nuxt dev",
    "dev:capacitor": "cross-env DISABLE_SSR=true DISABLE_PWA=true ENABLE_HTTPS=false nuxt generate",
    "build": "cross-env DISABLE_SSR=false DISABLE_PWA=false nuxt build",
    "generate:pwa": "cross-env DISABLE_SSR=true DISABLE_PWA=false nuxt generate",
    "generate:capacitor": "cross-env DISABLE_SSR=true DISABLE_PWA=true nuxt generate",
    "lint": "eslint app --fix",
    "format": "prettier -w app --ignore-path .gitignore"
  },
  "dependencies": {
    "@capacitor-community/privacy-screen": "^5.0.0",
    "@capacitor-community/stripe": "^5.5.2",
    "@capacitor-firebase/messaging": "^6.1.0",
    "@capacitor/android": "^6.1.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.1.0",
    "@capacitor/ios": "^6.1.0",
    "@directus/sdk": "^16.1.1",
    "@internationalized/date": "^3.5.4",
    "@nuxt/eslint": "^0.3.13",
    "@radix-icons/vue": "^1.0.0",
    "@stripe/stripe-js": "^4.1.0",
    "@tanstack/query-broadcast-client-experimental": "^5.50.1",
    "@tanstack/query-devtools": "^5.51.9",
    "@tanstack/query-persist-client-core": "^5.50.1",
    "@tanstack/query-sync-storage-persister": "^5.49.1",
    "@tanstack/vue-query": "^5.51.0",
    "@tanstack/vue-query-devtools": "^5.51.0",
    "@vee-validate/valibot": "^4.13.2",
    "@vueuse/core": "^10.11.0",
    "capacitor-native-biometric": "^4.2.2",
    "commitlint": "^19.3.0",
    "date-fns": "^3.6.0",
    "firebase": "^10.12.3",
    "lru-cache": "^11.0.0",
    "nuxt": "^3.12.3",
    "radix-vue": "^1.9.0",
    "socket.io-client": "^4.7.5",
    "stripe": "^16.2.0",
    "stripe-pwa-elements": "^2.1.0",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "uuid": "^10.0.0",
    "valibot": "^0.36.0",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.13.2",
    "vue": "latest",
    "vue-sonner": "^1.1.3",
    "vue3-lottie": "^3.3.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@iconify-json/bi": "^1.1.23",
    "@iconify-json/heroicons": "^1.1.21",
    "@iconify-json/line-md": "^1.1.38",
    "@iconify-json/lucide": "^1.1.199",
    "@nuxt/fonts": "^0.7.1",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.50.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/nuxt": "^0.9.1",
    "@vue/devtools-api": "^6.6.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "nuxt-directus-next": "^0.0.15",
    "nuxt-typed-router": "^3.6.5",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "sharp": "^0.33.4",
    "sharp-ico": "^0.1.5",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "vue-tsc": "^2.0.26"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 90,
    "bracketSameLine": true,
    "trailingComma": "none",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ],
    "**/*.{html,json,css,md,mdx}": [
      "prettier -w"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
